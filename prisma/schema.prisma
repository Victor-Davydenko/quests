generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quest {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  previewImg  String
  coverImg    String
  type        String
  level       String
  peopleCount Int[]
  duration    Int
}
